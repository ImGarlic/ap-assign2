<FlowPane alignment="CENTER" columnHalignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="300.0" stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dylan.dahub.controller.modal.ModalRequestController">
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="285.0" prefWrapLength="300.0" vgap="20.0">
         <children>
            <Label fx:id="message" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="285.0" text="Are you sure you want to delete this post?" textAlignment="CENTER" wrapText="true">
